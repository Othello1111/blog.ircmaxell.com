<% if (is_tag()) { %>
  <script>
    window.subData= { title:'Tag : <%= page.tag %>'}
  </script>
<% } else if (is_archive()) { %>
  <script>
    window.subData= { title:'Year : <%= page.year %><%= page.month ? '.' + page.month : '' %>'}
  </script>
<% } else if (is_category()) { %>
  <script>
    window.subData= { title:'Category : <%= page.category %>'}
  </script>
<% } %>
<section class="post-list">
	<% 
    var i = 0;
    page.posts.each(function(post){ %>
    <div class='post-wrapper'>
      <%- partial('post', {post: post}) %>
    </div>
  <% 
      if (i++ === 0) {
        %>
        <div class='post-wrapper'>
          <article class="post noreveal">
            <ins class="adsbygoogle"
                 style="display:block"
                 data-ad-format="fluid"
                 data-ad-layout-key="-ej+6g-15-c4+qd"
                 data-ad-client="ca-pub-8322654260452484"
                 data-ad-slot="1226052734"></ins>
              <script>
                  (adsbygoogle = window.adsbygoogle || []).push({});
              </script>
          </article>
        </div>
        <%
      }

    }) %>
</section>

<% if (page.total > 1) { %>
  <nav id="page-nav">
    <% if (page.prev != 0) { %>
    <a class="prev" rel="prev" href="<%= url_for(page.prev_link) %>">
      <span class="icon icon-chevron-left"></span>
      <span class="text">Previous</span>
    </a>
    <% } %>
    <% if (page.next != 0) { %>
    <a class="next" rel="next" href="<%= url_for(page.next_link) %>">
      <span class="text">Next</span>
      <span class="icon icon-chevron-right"></span>
    </a>
    <% } %>
  </nav>
  
<% } %>